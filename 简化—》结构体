#define _CRT_SECURE_NO_DEPRECATE 

#include <stdio.h>

//创建一个结构体类型
struct Book
{
	char name[200];//c语言程序设计
	short price;//55元
};
int main()
{
	//利用结构体类型 创建一个该类型的结构体变量
	struct Book b1 = { "c语言程序设计",0 };
	struct Book* bp = &b1;
	printf("书名= %s\n", bp->name);
	printf("价格= %d\n", bp->price);



	return 0;

}
